{
  "$schema": "https://raw.githubusercontent.com/microsoft/pyright/main/packages/pyright-internal/src/schemas/pyrightconfig.schema.json",
  "venvPath": ".",
  "venv": ".venv",
  "pythonVersion": "3.11",
  "pythonPlatform": "Linux",
  "typeCheckingMode": "basic",
  "exclude": [
    "external",
    ".git",
    "__pycache__",
    ".venv",
    ".pytest_cache",
    ".ruff_cache",
    ".pyright",
    ".pyright_cache",
    "typings",
    "node_modules",
    "**/__pycache__",
    "**/.pytest_cache",
    ".cache"
  ],
  "ignore": [
    "models/doclayout_yolo.py"
  ],
  "extraPaths": [
    "pipeline",
    "models"
  ]
}
